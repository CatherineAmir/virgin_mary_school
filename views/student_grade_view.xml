<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>



        <record id="vm_student_grade_tree_view" model="ir.ui.view">
            <field name="name">vm.student_grade.list</field>

            <field name="model">vm.student.grade</field>
            <field name="arch" type="xml">
                <list string="Student Grade" editable="bottom">
                    <field name="name"/>

                </list>
            </field>
        </record>



        <record id="student_grade_act_window" model="ir.actions.act_window">
            <field name="name">Student Grades</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vm.student.grade</field>

            <field name="view_mode">list</field>

        </record>



    </data>
</odoo>